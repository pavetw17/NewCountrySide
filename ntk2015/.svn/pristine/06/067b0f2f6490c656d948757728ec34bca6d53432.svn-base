package com.cwrs.ntk.common;

import java.awt.Color;

public class HTRange {
	public float min;
	public float max;
	public float value; 
	public String value_s; 
	public Color color;
	public int index;
	public HTRange()
	{
		value = 0.0f;
		min = 0;
		max = 0;
		color = new Color(0,0,0);
		index  = 0;
	}
	public HTRange(float mi, float ma, float v, Color c, int idx)
	{
		value = v;
		min = mi;
		max = ma;
		color = c;
		index  = idx;
	}
}
