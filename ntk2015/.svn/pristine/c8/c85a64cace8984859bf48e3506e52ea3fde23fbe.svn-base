package com.cwrs.ntk.servlets.dao;

public class ObjectKV {
   public  Object key;
   public  Object value;
   public boolean isThis = false;
   public ObjectKV(String k, String v){
	   key = k;
	   value = v;
   }
   public ObjectKV(String k, String v, boolean b){
	   key = k;
	   value = v;
	   isThis = b;
   }
   public void setSelected(boolean b){
	   this.isThis = b;
   }
}
